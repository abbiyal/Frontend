<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="medmart.loginmedmart">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="LoginMedMart"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoginMedMart">
        <activity android:name=".ForgotPassword.ForgetPasswordSuccessMessage">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".ForgotPassword.SetNewPassword"
            android:screenOrientation="portrait"/>
        <activity android:name=".ForgotPassword.VerifyOtp"
            android:screenOrientation="portrait"/>
        <activity android:name=".ForgotPassword.ForgetPassword"
            android:screenOrientation="portrait"/>
        <activity android:name=".Stub"
            android:screenOrientation="portrait"/>
        <activity android:name=".SignUp"
            android:screenOrientation="portrait"/>
        <activity android:name=".LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>